    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package Public",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "./v2/cmd/portfolio-downloader",
            "args": ["--debug"],
            "cwd": "${workspaceFolder}/v2"
        },
        {
            "name": "Launch Package Dev",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "./cmd/portfoliodownloader/dev",
            "args": ["--trace"],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Launch Package Dev with local responses",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "./cmd/portfoliodownloader/dev",
            "args": ["-l", "--trace"],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Launch Package Example Generator",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "./cmd/example-generator",
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}