{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/dhojayev/traderepublic-portfolio-downloader/refs/heads/main/v2/pkg/traderepublic/schemas/common.json",
  "definitions": {
    "uuid": {
      "type": "string",
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "money": {
      "type": "string",
      "pattern": "^[+-]?[0-9]+,[0-9]{2} [â‚¬$]$"
    },
    "percentage": {
      "type": "string",
      "pattern": "^[0-9]+,[0-9]{2} %$"
    },
    "icon": {
      "type": "string",
      "pattern": "^logos/([A-Z0-9]+|timeline_interest_new|merchant-[a-f0-9-]+|XF[0-9A-Z]+|contacts-[A-Z]-[A-Za-z]+|bank_[a-z0-9_]+)/v2$"
    }
  }
}